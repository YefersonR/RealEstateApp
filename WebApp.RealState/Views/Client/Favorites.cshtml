@model List<Core.Application.DTOS.Estates.FavoriteRequest>

<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">TipoDePropiedad</th>
                <th scope="col">ImagenDeLaPropiedad</th>
                <th scope="col">CodigoDeLaPropiedad</th>
                <th scope="col">TipoDeVenta</th>
                <th scope="col">ValorDeLaPropiedad</th>
                <th scope="col">CantidadDeHabitaciones</th>
                <th scope="col">CantidadDeBaños</th>
                <th scope="col">Tamaño</th>
                <th scope="col">Info</th>
                <th scope="col">DellToFav</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var data in Model){
                <tr>
                    <th scope="row">@data.Estates.Code</th>
                    <td>@data.Estates.EstateTypes.Name</td>
                    <td>@data.Estates.EstatesImgs.FirstOrDefault().ImgUrl</td>
                    <td>@data.Estates.Code</td>c
                    <td>@data.Estates.SellTypes.Name</td>
                    <td>@data.Estates.Price</td>
                    <td>@data.Estates.Rooms</td>
                    <td>@data.Estates.Toilets</td>
                    <td>@data.Estates.Area</td>
                    <td><a class="btn" asp-controller="General" asp-action="Estate" asp-route-code="@data.Estates.Code"> Info </a></td>
                    <td><a class="btn" asp-controller="Client" asp-action="DellFavorite" asp-route-FavId="@data.Id"> Delete </a></td>
                </tr>
            }
        </tbody>
     </table>
</div>